{
    "name": "rcrowe/twigbridge",
    "description": "Adds the power of Twig to Laravel",
    "keywords": ["laravel", "twig"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Crowe",
            "email": "hello@vivalacrowe.com"
        }
    ],
    "require": {
        "twig/twig": "~1.12"
    },
    "require-dev": {
        "laravel/framework": "~4.1.0",
        "mockery/mockery": "~0.9.0",
        "phing/phing": "~2.7.0",
        "phpunit/phpunit": "~4.0.0",
        "squizlabs/php_codesniffer": "~1.5.0"
    },
    "suggest": {
        "twig/extensions": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "TwigBridge\\": "src",
            "TwigBridge\\Tests\\": "tests"
        }
    }
}
